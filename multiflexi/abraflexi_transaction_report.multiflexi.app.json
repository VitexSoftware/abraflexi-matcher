{
    "image": "",
    "name": "AbraFlexi transaction report",
    "description": "obtain AbraFlexi bank transaction report",
    "executable": "abraflexi-transaction-report",
    "setup": "",
    "cmdparams": "",
    "deploy": "apt install abraflexi-matcher",
    "homepage": "https:\/\/github.com\/VitexSoftware\/abraflexi-matcher\/",
    "requirements": ["AbraFlexi"],
    "ociimage": "",
    "uuid": "dd22e4a1-58b3-4e4b-b832-ad65ebcf4dc9",
    "topics": ["Bank", "AbraFlexi", "Check", "Json"],
    "environment": {
        "ABRAFLEXI_COMPANY": {
            "type": "string",
            "description": "",
            "defval": "demo_de",
            "required": true
        },
        "ABRAFLEXI_LOGIN": {
            "type": "string",
            "description": "AbraFlexi Login",
            "defval": "winstrom",
            "required": true
        },
        "ABRAFLEXI_PASSWORD": {
            "type": "string",
            "description": "AbraFlexi password",
            "defval": "winstrom",
            "required": true
        },
        "ABRAFLEXI_URL": {
            "type": "string",
            "description": "AbraFlexi Server URI",
            "defval": "https:\/\/demo.flexibee.eu:5434",
            "required": true
        },
        "ABRAFLEXI_BANK": {
            "type": "string",
            "description": "AbraFlexi Bank code",
            "defval": "BANKOVNÍ ÚČET",
            "required": true
        },
        "RESULT_FILE": {
            "type": "string",
            "description": "write output json data to",
            "defval": "transaction-report_{ABRAFLEXI_COMPANY}.json",
            "required": false
        },
        "EASE_LOGGER": {
            "type": "string",
            "description": "write log messages using",
            "defval": "console|syslog",
            "required": false
        },
        "LANG": {
            "type": "set",
            "description": "Locale for this application",
            "defval": "cs_CZ",
            "required": false,
            "options": [
                "cs_CZ",
                "en_US"
            ]
        },
        "REPORT_SCOPE": {
            "type": "text",
            "description": "Time scope of transactions downloaded",
            "defval": "yesterday",
            "required": true
        }
    },
    "multiflexi": "1.27"
}
