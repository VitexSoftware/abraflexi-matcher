{
    "name": "deb/flexibee-matcher",
    "description": "Invoice Matcher for FlexiBee written in PHP",
    "type": "project",
    "version": "0.8",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "config": {
        "vendor-dir": "/var/lib/flexibee-matcher"
    },
    "require": {
        "deb/flexibee-bricks": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHP",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/flexibee-bricks",
            "options": {
                "symlink": true
            }
        }
    ]
}
