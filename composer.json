{
    "name": "vitexsoftware/abraflexi-matcher",
    "description": "Reminder sender for AbraFlexi written in PHP",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "require": {
        "spojenet/flexibee": "dev-main"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "AbraFlexi\\Matcher\\": "src/AbraFlexi/Matcher/",
            "AbraFlexi\\Matcher\\Ui\\": "src/AbraFlexi/Matcher/Ui/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": ["vendor/vitexsoftware/ease-core/tests/src/Ease/"],
            "Test\\AbraFlexi\\": ["vendor/spojenet/flexibee/test/src/AbraFlexi/"],
            "Test\\AbraFlexi\\Matcher\\": "tests/src/AbraFlexi/Matcher/",
            "Test\\": "testing/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4@dev",
        "phpunit/php-code-coverage": "^10.1@dev"
    }
}
